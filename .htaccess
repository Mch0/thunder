<IfModule mod_rewrite.c>

   RewriteEngine on
	Options -Indexes
	RewriteCond %{HTTP_HOST} ^thunderbot\.gg$ [NC]
	RewriteRule ^.*$ http://www.thunderbot.gg%{REQUEST_URI} [R=301,L] # <-- Mind the 'L'!

   RewriteRule    ^$ app/webroot/    [L]
   RewriteRule    (.*) app/webroot/$1 [L]
</IfModule>

